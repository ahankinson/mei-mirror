<mei meiversion="2010-05" xmlns="http://www.music-encoding.org/ns/mei">
  <meihead>
    <filedesc>
      <titlestmt>
        <title>Variations for Piano op. 27, second movement</title>
        <respstmt>
          <resp>Composer</resp>
          <name>Anton Webern</name>
        </respstmt>
      </titlestmt>
      <pubstmt/>
    </filedesc>
    <profiledesc>
      <langusage>
        <language xml:id="it"/>
        <language xml:id="de"/>
      </langusage>
    </profiledesc>
  </meihead>
  <music>
    <body>
      <mdiv>
        <score>
          <scoredef meter.count="2" meter.unit="4">
            <staffgrp symbol="brace">
              <staffdef n="1" lines="5" clef.shape="G" clef.line="2"/>
              <staffdef n="2" lines="5" clef.shape="F" clef.line="4"/>
            </staffgrp>
          </scoredef>
          <section>
            <measure type="upbeat" complete="i">
              <staff n="1">
                <layer n="1">
                  <clefchange shape="F" line="4" tstamp="1"/>
                  <space dur="8"/>
                  <note xml:id="m0s1_n1" pname="g" oct="3" dur="8" accid="s"/>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <clefchange shape="G" line="2" tstamp="1"/>
                  <note xml:id="m0s2_n1" pname="b" oct="5" dur="8" accid="f"/>
                  <space dur="8"/>
                </layer>
              </staff>
              <beamspan staff="2 1" startid="m0s2_n1" endid="m0s1_n1"
                plist="m0s1_n1 m0s2_n1"/>
              <slur staff="1 2" startid="m0s2_n1" endid="m0s1_n1"
                curvedir="above"/>
              <dynam staff="1" place="below" plist="m0s1_n1 m0s2_n1" tstamp="1"
                >f</dynam>
            </measure>
          </section>
          <section>
            <measure n="1" left="rptstart">
              <staff n="1">
                <layer n="1">
                  <space dur="4"/>
                  <clefchange shape="G" line="2" tstamp="2"/>
                  <note xml:id="m1s1_n1" pname="a" oct="4" dur="8" accid="n"
                    artic="dot"/>
                  <rest dur="8"/>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <rest dur="8"/>
                  <note xml:id="m1s2_n1" pname="c" oct="3" dur="8" accid="n"
                    artic="dot"/>
                  <space dur="4"/>
                  <clefchange shape="F" line="4" tstamp="3"/>
                </layer>
              </staff>
              <beamspan staff="2 1" startid="m1s2_n1" endid="m1s1_n1"
                plist="m1s1_n1 m1s2_n1"/>
              <dynam staff="1" place="below" plist="m1s1_n1 m1s2_n1"
                tstamp="1.5">p</dynam>
            </measure>
            <measure n="2">
              <staff n="1">
                <layer n="1">
                  <rest dur="8"/>
                  <note xml:id="m2s1_n1" pname="f" oct="5" dur="4" accid="n"
                    artic="ten"/>
                  <rest dur="8"/>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <note xml:id="m2s2_n1" pname="c" oct="4" dur="4" accid="s"
                    artic="ten"/>
                  <rest dur="8"/>
                  <note xml:id="m2s2_n2" grace="acc" pname="b" oct="2" dur="8"
                    accid="n"/>
                  <note xml:id="m2s2_n3" pname="d" oct="3" dur="8" accid="n"
                    artic="dot"/>
                </layer>
              </staff>
              <beamspan staff="2 1" startid="m2s2_n3" endid="m3s1_n2"
                plist="m2s2_n3 m3s1_n2"/>
              <slur staff="2" curvedir="below" startid="m2s2_n2" endid="m2s2_n3"
                bulge="-2"/>
              <dynam staff="1" place="below" plist="m2s1_n1 m2s2_n1" tstamp="1"
                >f</dynam>
              <dynam staff="1" place="below"
                plist="m2s2_n2 m2s2_n3 m3s1_n1 m3s1_n2" tstamp="2.5">p</dynam>
            </measure>
            <measure n="3">
              <staff n="1">
                <layer n="1">
                  <note xml:id="m3s1_n1" grace="acc" pname="g" oct="6" dur="8"
                    accid="n"/>
                  <note xml:id="m3s1_n2" pname="e" oct="6" dur="8" accid="n"
                    artic="dot"/>
                  <rest dur="8"/>
                  <rest dur="4"/>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <rest dur="4"/>
                  <rest dur="8"/>
                  <clefchange shape="G" line="2" tstamp="2.5"/>
                  <chord xml:id="m3s2_c1" dur="8" artic="acc">
                    <note pname="f" oct="5" accid="n"/>
                    <note pname="c" oct="5" accid="n"/>
                    <note pname="f" oct="4" accid="s"/>
                  </chord>
                </layer>
              </staff>
              <beamspan staff="2 1" startid="m3s2_c1" endid="m4s1_c1"
                plist="m3s2_c1 m4s1_c1"/>
              <dynam staff="1" place="below" plist="m3s2_c1 m4s1_c1"
                tstamp="2.5">f</dynam>
              <slur staff="1" curvedir="above" startid="m3s1_n1" endid="m3s1_n2"
                bulge="2"/>
            </measure>
            <measure n="4">
              <staff n="1">
                <layer n="1">
                  <chord xml:id="m4s1_c1" dur="8" artic="acc">
                    <note pname="c" oct="5" accid="n"/>
                    <note pname="f" oct="4" accid="s"/>
                    <note pname="c" oct="4" accid="s"/>
                  </chord>
                  <rest dur="8"/>
                  <clefchange shape="F" line="4" tstamp="2"/>
                  <note xml:id="m4s1_n2" pname="d" oct="3" dur="8" accid="n"/>
                  <space dur="8"/>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <rest dur="4"/>
                  <space dur="8"/>
                  <note xml:id="m4s2_n1" pname="e" oct="6" dur="8" accid="n"/>
                </layer>
              </staff>
              <beamspan staff="2 1" beam.with="above" startid="m4s2_n1"
                endid="m4s1_n2" plist="m4s1_n2 m4s2_n1"/>
              <dynam staff="1" place="below" plist="m4s2_n1 m4s1_n2" tstamp="2"
                >p</dynam>
              <slur staff="1" curvedir="above" endid="m4s2_n1" startid="m4s1_n2"
              />
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
