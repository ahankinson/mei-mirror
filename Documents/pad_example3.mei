<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mei [
<!ENTITY % ISOlat1 SYSTEM 
   'http://text.lib.virginia.edu/charent/iso-lat1.ent'> %ISOlat1;
]>
<?oxygen SCHSchema="http://music-encoding.org/mei/schemata/2010-05/rng/mei-all.rng"?>
<?oxygen RNGSchema="http://music-encoding.org/mei/schemata/2010-05/rng/mei-all.rng"?>
<mei meiversion="2010-05" xmlns="http://www.music-encoding.org/ns/mei">
  <meihead>
    <altmeiid/>
    <filedesc>
      <titlestmt>
        <title>Sonata grande</title>
        <title type="uniform">Sonatas, piano, no. 21, op. 53, C
          major</title>
        <respstmt>
          <name>Beethoven, Ludwig van, 1770-1827</name>
        </respstmt>
      </titlestmt>
      <pubstmt>
        <geogname>Charlottesville, Virginia, USA</geogname>
        <respstmt>
          <name>Perry Roland</name>
        </respstmt>
      </pubstmt>
      <seriesstmt>
        <title>Paderborn examples</title>
        <identifier>no. 3</identifier>
      </seriesstmt>
    </filedesc>
  </meihead>
  <music>

    <!-- <facsimile>
      <graphic width="814" height="214" href="Beethoven.jpg"/>
    </facsimile> -->

    <body>
      <mdiv type="movement">
        <score>
          <scoredef key.sig="0" meter.count="4" meter.unit="4"
            key.mode="major" meter.sym="common">

            <!-- User-defined symbols -->
            <symboltable>
              <symboldef xml:id="usersym1">
                <line x="0" y="0" x2="0" y2="0" rend="narrow"/>
                <!-- Structured content, i.e., some combination of curve,
                  line, symbol, and anchoredtext elements, is preferred;
                  however, processing instructions may be used, e.g.,
                  <? processorName PostScript commands/SVG markup ?> -->
              </symboldef>
              <symboldef xml:id="usersym2">
                <line x="0" y="0" x2="0" y2="0" rend="narrow"/>
                <line x="0" y="0" x2="0" y2="0" rend="narrow"/>
              </symboldef>
            </symboltable>

            <!-- Imitation of the usual forme-work found at the top of page 1
              of printed music. -->
            <pghead1>
              <table>
                <tr>
                  <td/>
                  <td>
                    <title>Sonata grande</title>
                  </td>
                  <td n="composer">
                    <!-- The del element is used for indicating a textual/musical deletion. -->
                    <del rend="strike">L. v. Beethoven</del>
                  </td>
                </tr>
              </table>
            </pghead1>

            <staffgrp barthru="true">
              <staffdef n="1" lines="5" clef.line="2" clef.shape="G"
                key.sig="0"/>
              <staffdef n="2" lines="5" clef.line="4" clef.shape="F"
                key.sig="0"/>
            </staffgrp>
          </scoredef>
          <section>
            <measure n="1">
              <staff n="1">

                <layer>
                  <!-- A staff might *explicitly* contain no events, not even
                a rest. Also, if there's only 1 layer, then the layer element
                may be omitted. -->
                  <mspace/>
                </layer>

              </staff>
              <staff n="2">
                <layer>
                  <pad num="5"/>
                  <rest dur="8"/>
                  <beam>
                    <chord dur="8">
                      <note pname="c" oct="3"/>
                      <note pname="e" oct="3"/>
                    </chord>
                    <chord dur="8">
                      <note pname="c"/>
                      <note pname="e"/>
                    </chord>

                    <!-- Exact repetition of the preceding chord can be indicated
                      with an empty chord element. -->
                    <chord/>

                  </beam>
                  <chord dur="2" stem.mod="1slash">
                    <note pname="c"/>
                    <note pname="e"/>
                  </chord>
                </layer>
                <layer>
                  <beam>
                    <note pname="c" oct="2" dur="8"/>
                    <chord>
                      <note pname="c"/>
                      <note pname="g"/>
                    </chord>
                    <chord>
                      <note pname="c"/>
                      <note pname="g"/>
                    </chord>
                    <chord>
                      <note pname="c"/>
                      <note pname="g"/>
                    </chord>
                  </beam>
                  <chord dur="2" stem.mod="1slash">
                    <note pname="c"/>
                    <note pname="g"/>
                  </chord>
                </layer>
              </staff>
              <tempo place="above" staff="1" tstamp="1"><title
                  type="mvt">Allegro con Brio</title>.</tempo>
              <dynam place="above" staff="2" tstamp="1" vo="-5"
                ho="-7">

                <!-- The rend element is used for capturing typographic
                details of text. -->
                <rend rend="dblunderline">pp:</rend>

              </dynam>
              <dynam place="below" staff="2" tstamp="1" ho="-7">
                <rend rend="dblunderline">pp:</rend>
              </dynam>
            </measure>
            <measure n="2" right="rptstart">
              <staff n="1">
                <layer>
                  <mspace/>
                </layer>
              </staff>
              <staff n="2">
                <layer>
                  <chord dur="2" stem.mod="1slash">
                    <note pname="c" oct="3"/>
                    <note pname="e"/>
                  </chord>
                  <chord dur="4" stem.mod="1slash">
                    <note pname="c"/>
                    <note pname="e"/>
                  </chord>
                  <chord dur="4" stem.mod="1slash">
                    <note pname="d"/>
                    <note pname="f" accid="s"/>
                  </chord>
                </layer>
                <layer>
                  <chord dur="2" stem.mod="1slash">
                    <note pname="c" oct="2"/>
                    <note pname="g"/>
                  </chord>
                  <chord dur="4" stem.mod="1slash">
                    <note pname="c"/>
                    <note pname="g"/>
                  </chord>
                  <chord dur="4" stem.mod="1slash">
                    <note pname="c"/>
                    <note pname="a"/>
                  </chord>
                </layer>
              </staff>
            </measure>
            <measure n="3">
              <staff n="1">
                <layer>
                  <mspace/>
                </layer>
              </staff>
              <staff n="2">

                <!-- Layers may be encoded 'out of order' but then @def should be
                     supplied. -->
                <layer n="2">
                  <chord dur="2" stem.mod="1slash">
                    <note pname="b" oct="1"/>
                    <note pname="g" oct="2"/>
                  </chord>
                  <halfmrpt altsym="usersym1"/>
                </layer>
                <layer n="1">
                  <chord dur="4" dots="1">
                    <note pname="d" oct="3"/>
                    <note pname="g"/>
                  </chord>
                  <beam>
                    <note pname="b" dur="16"/>
                    <note pname="a"/>
                  </beam>
                  <note pname="g" dur="8" stem.dir="up"/>
                  <rest dur="8"/>

                  <!-- @next may be used to indicate voice leading. -->
                  <rest dur="4" next="n1"/>

                </layer>

              </staff>
              <phrase curvedir="above" staff="2" tstamp="1" dur="0m+3"/>
            </measure>
            <measure n="4">
              <staff n="1">
                <layer>
                  <note xml:id="n1" grace="acc" pname="c" accid="s"
                    oct="6" dur="16"/>
                  <note pname="d" dur="4" tie="i"/>
                  <beam>
                    <note pname="d" dur="16" tie="t"/>
                    <note pname="c" accid="n"/>
                    <note pname="b" oct="5"/>
                    <note pname="a"/>
                  </beam>
                  <note pname="g" dur="4" artic="stroke"/>
                  <rest/>
                </layer>
              </staff>
              <staff n="2">
                <!-- What do the repeat signs refer to: the printed or the performed notes? -->
                <layer>
                  <choice>
                    <orig>
                      <halfmrpt altsym="usersym1"/>
                      <halfmrpt altsym="usersym1"/>
                    </orig>
                    <choice>
                      <reg n="print">
                        <chord dur="2" stem.mod="1slash">
                          <note pname="b" oct="2"/>
                          <note pname="g" oct="3"/>
                        </chord>
                        <chord dur="2" stem.mod="1slash">
                          <note pname="b" oct="2" dur="4"/>
                          <note pname="g" oct="3" dur="2"/>
                        </chord>
                      </reg>
                      <reg n="performance">
                        <chord dur="8">
                          <note pname="b" oct="2"/>
                          <note pname="g" oct="3"/>
                        </chord>
                        <chord/>
                        <chord/>
                        <chord/>
                        <chord dur="8">
                          <note pname="b" oct="2"/>
                          <note pname="g" oct="3"/>
                        </chord>
                        <chord/>
                        <chord/>
                        <chord/>
                      </reg>
                    </choice>
                  </choice>
                </layer>
              </staff>
              <phrase curvedir="above" staff="1" tstamp="1" dur="0m+3"/>

              <!-- Page-anchored text, rotated, containing a line break.
              The x and y attributes are required, but may be left empty. -->
              <anchoredtext n="directive" x="0" y="0">
                <rend rotation="90">D&auml;mpfung<lb/>
                  augehoben</rend>
              </anchoredtext>
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
  </music>
</mei>
