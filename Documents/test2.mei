<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE section SYSTEM "../mei19.1/DTD/mei19-all.dtd" [
  <!ENTITY % meiCMN 'INCLUDE'>
  <!ENTITY % meiCritApp 'INCLUDE'>
  <!ENTITY m3 '<measure n="3"/>'>
  <!ENTITY m4 '<measure n="4"/>'>
  <!ENTITY m5 '<measure n="5"/>'>
  <!ENTITY m6 '<measure n="6"/>'>
]>

<section>
  <app>
    <!-- source 1 -->
    <rdg>
      <!-- in both versions -->
      <measure n="1"/>
      <app>
        <!-- the differences start here -->
        <rdg>
          <measure n="2"><!-- contents here --></measure> &m3;
          &m4; &m5; &m6; </rdg>
        <rdg>
          <measure n="2"
            ><!-- contents deleted but meas. retained because pointer to image is
        req'd --></measure>
          <measure n="3"/>
          <measure n="4"/>
          <app>
            <rdg>
              <!-- old order -->
              <measure n="5"/>
              <measure n="6"/>
            </rdg>
            <rdg>
              <!-- new order -->
              <measure n="6"/>
              <measure n="5"/>
            </rdg>
          </app>
          <measure n="7"/>
          <!-- added measures -->
          <measure n="8"/>
        </rdg>
      </app>
      <!-- continue with measures common to both versions -->
    </rdg>
    <!-- source 2 -->
    <rdg> </rdg>
  </app>
</section>
