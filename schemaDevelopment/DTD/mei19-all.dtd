<!ENTITY % content.treatmenthist '(#PCDATA|%model.textphraseLike.limited;)*'>
<!ELEMENT %n.treatmenthist; %content.treatmenthist;>
<!ATTLIST %n.treatmenthist;
          %att.common;
          %att.bibl;>
]]>

