<!ENTITY % physloc 'IGNORE'>
<![%physloc;[
<!ENTITY % content.physloc '(#PCDATA|%model.textphraseLike.limited;)*'>
<!ELEMENT %n.physloc; %content.physloc;>
<!ATTLIST %n.physloc;
          %att.common;
          %att.bibl;>
