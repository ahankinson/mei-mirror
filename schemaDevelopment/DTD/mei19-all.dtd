          accid              %data.ACCIDENTAL.IMPLICIT; #IMPLIED
          fing               (x|o|1|2|3|4|5)            #IMPLIED
          fret               (1|2|3|4|5)                #IMPLIED
          inth               %data.INTEGERNONNEGATIVE;  #IMPLIED
