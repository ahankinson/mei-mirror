                     key.mode           %data.MODE;            %INHERITED;
                     key.pname          %data.PITCHNAME;       #IMPLIED
                     key.sig            %data.KEYSIGNATURE;    %INHERITED;
                     key.sig.mixed      CDATA                  %INHERITED;
                     meter.count        %data.NUMBER;          #IMPLIED
