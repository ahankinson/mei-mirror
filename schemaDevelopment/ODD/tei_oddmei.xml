     <body>
       <head>TEI with MEI</head>
       <p>This TEI customization describes a schema that includes
       <emph>all</emph> of the TEI (P5) modules and some MEI ones.
       </p>
       <schemaSpec ident="tei_mei" start="TEI teiCorpus">
         <moduleRef key="tei"/>                              <!-- required -->
         <moduleRef key="core"/>                             <!-- required -->
         <moduleRef key="analysis"/>
         <moduleRef key="certainty"/>
         <moduleRef key="corpus"/>
         <moduleRef key="dictionaries"/>
         <moduleRef key="drama"/>
         <moduleRef key="figures"/>
         <moduleRef key="gaiji"/>
         <moduleRef key="header"/>                           <!-- required -->
         <moduleRef key="iso-fs"/>
         <moduleRef key="linking"/>
         <moduleRef key="msdescription"/>
         <moduleRef key="namesdates"/>
         <moduleRef key="nets"/>
         <moduleRef key="spoken"/>
         <moduleRef key="textcrit"/>
         <moduleRef key="textstructure"/>                    <!-- required -->
         <moduleRef key="transcr"/>
         <moduleRef key="verse"/>
	 <moduleRef key="tagdocs"/>
	 
        <elementRef  key="mdiv" source="file:///Users/raffaeleviglianti/Documents/Research/MEI/branches/schemaDevelopment/ODD/mei-source.xml" />
      
       	<elementSpec module="MEI.shared" mode="change" ident="mdiv" ns="http://www.music-encoding.org/ns/mei">
       		<classes>
       			<memberOf key="model.divLike"/>
       		</classes>
	   </elementSpec>

       </schemaSpec>
     </body>
   </text>
</TEI>
