    name="att.internetMedia.attribute.mimeType">
    <optional>
      <attribute xmlns:sch="http://purl.oclc.org/dsdl/schematron"
        xmlns="http://relaxng.org/ns/structure/1.0" name="mimeType">
        <a:documentation
          xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(MIME
          media type) specifies the applicable multimedia internet mail
          extension (MIME) media type</a:documentation>
        <data type="token">
          <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
        </data>
