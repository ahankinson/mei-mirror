  <define name="data.CLUSTER">
    <a:documentation>Tone-clusters</a:documentation>
    <choice>
      <value>whbox</value>
      <value>blbox</value>
    </choice>
  </define>

  <define name="data.CURVERENDITION">
    <a:documentation>Renderings of curves</a:documentation>
    <choice>
      <value>medium</value>
      <value>wide</value>
      <value>dashed</value>
      <value>dotted</value>
