  <define name="data.MIDITEMPO">
    <a:documentation>MIDI quarter notes per minute: positive integer in the
      range 10-1000</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">10</param>
      <param name="maxInclusive">1000</param>
    </data>
  </define>

  <define name="data.MODUSMAIOR">
    <a:documentation>Maxima-long relationship values.</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">2</param>
      <param name="maxInclusive">3</param>
    </data>
  </define>

  <define name="data.MODUSMINOR">
    <a:documentation>Long-breve relationship values.</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">2</param>
      <param name="maxInclusive">3</param>
    </data>
  </define>

  <define name="data.OCTAVE.DIS">
    <a:documentation>The amount of octave displacement; that is, '8' (as in
      '8va' for 1 octave), '15' (for 2 octaves), or rarely '22' (for 3
      octaves).</a:documentation>
    <data type="positiveInteger">
      <param name="pattern">8|15|22</param>
    </data>
  </define>

  <define name="data.ORIENTATION">
    <a:documentation>Rotation or reflection of base symbol
      values.</a:documentation>
    <data type="token">
      <param name="pattern">reversed|90CW|90CCW</param>
    </data>
  </define>

  <define name="data.PAGE.PANELS">
    <a:documentation>The number of panels per page.</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">1</param>
      <param name="maxInclusive">2</param>
    </data>
  </define>

  <define name="data.PERCENT">
    <a:documentation>Positive decimal number plus '%', i.e.,
      [0-9]+(\.?[0-9]*)?\%</a:documentation>
    <data type="token">
      <param name="pattern">[0-9]+(\.?[0-9]*)?%</param>
    </data>
  </define>

  <define name="data.PROLATIO">
    <a:documentation>Semibreve-minim relationship values.</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">2</param>
      <param name="maxInclusive">3</param>
    </data>
  </define>

  <define name="data.RATIO">
    <a:documentation>A ratio, i.e., [0-9]+(\.?[0-9]*)?:[0-9]+(\.?[0-9]*)? For
      example, "40:7.2319"</a:documentation>
    <data type="token">
      <param name="pattern">[0-9]+(\.?[0-9]*)?:[0-9]+(\.?[0-9]*)?</param>
    </data>
  </define>

  <define name="data.SLASH">
    <a:documentation>The number of slashes to be rendered for
      tremolandi.</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">1</param>
      <param name="maxInclusive">6</param>
    </data>
  </define>

  <define name="data.STRINGNUMBER">
    <a:documentation>In string tablature, the number of the string to be played,
      i.e., [1-9]+.</a:documentation>
    <data type="positiveInteger"/>
  </define>

  <define name="data.TEMPOVALUE">
    <a:documentation>Beats (meter signature denominator) per minute, e.g.
      120.</a:documentation>
    <data type="positiveInteger"/>
  </define>

  <define name="data.TEMPUS">
    <a:documentation>Breve-semibreve relationship values</a:documentation>
    <data type="positiveInteger">
      <param name="minInclusive">2</param>
      <param name="maxInclusive">3</param>
    </data>
  </define>

  <define name="data.TSTAMPOFFSET">
    <a:documentation>A positive or negative offset from the value given in the
      tstamp attribute in terms of musical time, i.e., beats[.fractional beat
      part]</a:documentation>
    <data type="decimal"/>
  </define>

  <define name="data.URI">
    <a:documentation>a Uniform Resource Identifier, see
      [RFC2396]</a:documentation>
    <data type="anyURI"/>
  </define>

  <define name="data.ACCIDENTAL.EXPLICIT">
    <a:documentation>Accidental attribute values: s = sharp, f = flat, ss =
      dblsharp, x=dblsharp, ff = dblflat, xs = triple sharp, tb = triple flat, n
      = natural, nf = naturalflat, ns = naturalsharp. ss indicates the use of 2
      sharp signs, while x indicates the use of a single double sharp. nf and ns
      are used to cancel dbflats and dblsharps, respectively. su = sharp note
      qtr. tone up, sd = sharp note qtr. tone down, fu = flat note qtr. tone up,
      fd = flat note qtr. tone down, nu = natural note qtr. tone up, nd =
      natural note quarter tone down.</a:documentation>
    <choice>
      <value>s</value>
      <value>f</value>
      <value>ss</value>
      <value>x</value>
      <value>ff</value>
      <value>xs</value>
      <value>tb</value>
      <value>n</value>
      <value>nf</value>
      <value>ns</value>
      <value>su</value>
      <value>sd</value>
      <value>fu</value>
      <value>fd</value>
      <value>nu</value>
      <value>nd</value>
    </choice>
  </define>

  <define name="data.ACCIDENTAL.IMPLICIT">
    <a:documentation>Accidental attribute values: s = sharp, f = flat, ss =
      dblsharp, x=dblsharp, ff = dblflat, n = natural.</a:documentation>
    <choice>
      <value>s</value>
      <value>f</value>
      <value>ss</value>
      <value>ff</value>
      <value>n</value>
    </choice>
  </define>

  <define name="data.ARTICULATION">
    <a:documentation>The following list of articulations mostly corresponds to
      symbols 1D110-1D111, 1D17B-1D182, 1D185-1D189, 1D1AA-1D1AD, 1D1B3-1D1B5
      from the Western Musical Symbols portion of the Unicode Standard, v. 3.1.
      The dot and stroke values may be used in cases where interpretation is
      difficult or not desirable.</a:documentation>
    <choice>
      <value>acc</value>
      <value>stacc</value>
      <value>ten</value>
      <value>stacciss</value>
      <value>marc</value>
      <value>marc-stacc</value>
      <value>acc-marc</value>
      <value>spicc</value>
      <value>rip</value>
      <value>doit</value>
      <value>plop</value>
      <value>fall</value>
      <value>bend</value>
      <value>flip</value>
      <value>smear</value>
      <value>dnbow</value>
      <value>upbow</value>
      <value>harm</value>
      <value>snap</value>
      <value>fingernail</value>
      <value>ten-stacc</value>
      <value>damp</value>
      <value>dampall</value>
      <value>open</value>
      <value>stop</value>
      <value>dbltongue</value>
      <value>trpltongue</value>
      <value>heel</value>
      <value>toe</value>
      <value>tap</value>
      <value>lhpizz</value>
      <value>dot</value>
      <value>stroke</value>
    </choice>
  </define>

  <define name="data.ARTICULATIONS">
    <a:documentation>One or more values from the list given in the definition of
      data.ARTICULATION.</a:documentation>
    <list>
      <oneOrMore>
        <ref name="data.ARTICULATION"/>
      </oneOrMore>
    </list>
  </define>

  <define name="data.AUGMENTDOT">
    <a:documentation>Dots attribute values (number of augmentation dots) (Read,
      113-119, ex. 8-21)</a:documentation>
    <data type="nonNegativeInteger">
      <param name="maxInclusive">4</param>
    </data>
  </define>

  <define name="data.BARPLACE">
    <a:documentation>Placement of barlines: 'mensur' = between staves only,
      'staff' = between and across staves as necessary, 'takt' = short line
      above staff or through top line. The default value is
      'staff'.</a:documentation>
    <choice>
      <value>mensur</value>
      <value>staff</value>
      <value>takt</value>
    </choice>
  </define>

  <define name="data.BARRENDITION">
    <a:documentation>Renderings of barlines</a:documentation>
    <choice>
      <value>dashed</value>
      <value>dotted</value>
      <value>dbl</value>
      <value>dbldashed</value>
      <value>dbldotted</value>
      <value>end</value>
      <value>invis</value>
      <value>rptstart</value>
      <value>rptboth</value>
      <value>rptend</value>
      <value>single</value>
    </choice>
  </define>

  <define name="data.BEAM">
    <a:documentation>Beam attribute values: initial, medial, terminal. Nested
      beaming is permitted.</a:documentation>
    <data type="token">
      <param name="pattern">[i|m|t][1-6]</param>
    </data>
  </define>

  <define name="data.BEAMS">
    <a:documentation>One or more from the list given in the definition of the
      BEAM datatype.</a:documentation>
    <list>
      <oneOrMore>
        <ref name="data.BEAM"/>
      </oneOrMore>
    </list>
  </define>

  <define name="data.BOOLEAN">
    <a:documentation>Boolean attribute values</a:documentation>
    <choice>
      <value>true</value>
      <value>false</value>
    </choice>
  </define>

  <define name="data.CERTAINTY">
    <a:documentation>Values for certainty attribute</a:documentation>
    <choice>
      <value>high</value>
      <value>medium</value>
      <value>low</value>
      <value>unknown</value>
    </choice>
  </define>

  <define name="data.CLEFLINE">
    <a:documentation>Clef line attribute values</a:documentation>
    <data type="positiveInteger"/>
  </define>

  <define name="data.CLEFSHAPE">
    <a:documentation>Clef shape attribute values (Read,
      p.53-56)</a:documentation>
    <choice>
      <value>G</value>
      <value>GG</value>
      <value>F</value>
      <value>C</value>
      <value>perc</value>
      <value>TAB</value>
