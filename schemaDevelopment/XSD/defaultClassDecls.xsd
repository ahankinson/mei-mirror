    <xs:attributeGroup ref="att.common.anl"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.octave.ges">
    <xs:attributeGroup ref="att.duration.performed"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.octave.log">
    <xs:attributeGroup ref="att.controlevent"/>
    <xs:attributeGroup ref="att.octavedisplacement"/>
    <xs:attributeGroup ref="att.startendid"/>
    <xs:attributeGroup ref="att.duration.timestamp"/>
    <xs:attribute name="coll">
      <xs:annotation>
        <xs:documentation>encodes whether the octave displacement
