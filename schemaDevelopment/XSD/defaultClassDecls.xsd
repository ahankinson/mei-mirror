    <xs:attribute name="octave.default" type="data.OCTAVE">
      <xs:annotation>
        <xs:documentation>contains a default octave specification
          for use when the first note, rest, chord, etc. in a measure does not
          have an octave value specified.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.octavedisplacement">
    <xs:attribute name="dis" type="data.OCTAVE.DIS">
      <xs:annotation>
