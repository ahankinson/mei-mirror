    <xs:attributeGroup ref="att.accidental.performed"/>
    <xs:attributeGroup ref="att.articulation.performed"/>
    <xs:attributeGroup ref="att.duration.performed"/>
    <xs:attributeGroup ref="att.instrumentident"/>
    <xs:attribute name="oct.ges" type="data.OCTAVE">
      <xs:annotation>
