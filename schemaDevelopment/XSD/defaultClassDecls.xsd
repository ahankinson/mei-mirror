  <xs:attributeGroup name="att.slur.ges">
    <xs:attributeGroup ref="att.duration.performed"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="att.slur.log">
    <xs:attributeGroup ref="att.controlevent"/>
