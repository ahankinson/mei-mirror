    <xs:attribute name="page.width" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the physical width of the
          rendered output page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.units" type="data.PGUNITS">
      <xs:annotation>
        <xs:documentation>contains the real-world measurement
          units (inches, centimeters, millimeters) used to describe the rendered
          page height, width, and margins.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.topmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at
          the top of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.botmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at
          the bottom of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.leftmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at
          the left side of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.rightmar" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>indicates the amount of whitespace at
          the right side of a rendered score page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.panels" type="data.PAGE.PANELS">
      <xs:annotation>
        <xs:documentation>indicates the number of logical pages to
          be rendered on a single physical page.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="page.scale" type="data.PGSCALE">
      <xs:annotation>
        <xs:documentation>indicates how the page should be scaled
          when rendered.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.packexp" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes a note's spacing relative to
          its time value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.packfact" type="xs:decimal">
      <xs:annotation>
        <xs:documentation>describes the note spacing of
          output.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="spacing.staff" type="xs:decimal">
