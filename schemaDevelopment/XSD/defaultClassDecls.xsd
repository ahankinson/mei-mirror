    <xs:attributeGroup ref="att.duration.default"/>
    <xs:attributeGroup ref="att.keysigs.log"/>
    <xs:attributeGroup ref="att.meters.log"/>
    <xs:attributeGroup ref="att.octavedefault"/>
    <xs:attributeGroup ref="att.transposition"/>
    <xs:attributeGroup ref="att.beaming.log"/>
    <xs:attributeGroup ref="att.mensuration.log"/>
