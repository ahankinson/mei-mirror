  <xs:simpleType name="data.ARTICULATIONS">
    <xs:annotation>
      <xs:documentation>One or more values from the list given in the definition of
      data.ARTICULATION.</xs:documentation>
    </xs:annotation>
    <xs:restriction>
      <xs:simpleType>
        <xs:list itemType="mei:data.ARTICULATION"/>
